#################
#  ZSH options  #
#################
HISTFILE=~/.zsh_history
export HISTSIZE=
export HISTFILESIZE=
export SAVEHIST=
setopt no_list_beep
setopt no_beep
setopt appendhistory
setopt hist_ignore_all_dups
setopt hist_reduce_blanks
setopt autocd
setopt prompt_subst
setopt autopushd

unsetopt correctall
